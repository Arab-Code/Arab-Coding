مهمة تنفيذ1(القيمة){
متغير الناتج=جسون(القيمة)؛
الناتج=الناتج.العنصر("القيمة")؛
متغير النتيجة=الصفحة.العنصر("النتيجة")؛
النتيجة.المحتوى=الناتج؛
}

مهمة نقر(المدخل){
متغير النتيجة=الصفحة.العنصر("النتيجة")؛
متغير م=متغيرات_المهمة()؛
   م.اضافة(المدخل)؛
    م.اضافة(النتيجة.المحتوى)؛
 
  مهمةخدمة("الخدمة.عرب",م.المحتوى(),تنفيذ1) ؛


}
